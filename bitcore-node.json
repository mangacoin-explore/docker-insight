{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api-monacoin",
    "insight-ui-monacoin",
    "web"
  ],
  "servicesConfig": {
    "insight-ui-monacoin": {
      "routePrefix": ""
    }
  },
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/lib/node_modules/monacore-node/bin/monacoind"
      }
    }
  }
}
