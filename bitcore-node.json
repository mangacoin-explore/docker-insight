{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "@mangacoin-explore/insight-api-mxxgacoin",
    "@mangacoin-explore/insight-ui-mxxgacoin",
    "web"
  ],
  "servicesConfig": {
    "insight-ui-mxxgacoin": {
      "routePrefix": ""
    },
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/lib/node_modules/monacore-node/bin/monacoind"
      }
    }
  }
}
